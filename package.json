{
  "name": "mindx-denise",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && vite build",
    "preview": "vite preview",
    "start:dev": "vite",
    "css": "tailwindcss -o ./public/tailwind.css",
    "copy-static": "rm -rf ./public/static/assets && cp -r ./node_modules/@mx/denise-core/dist/static ./public",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prettier": "prettier --write '{src,__{tests,mocks}__}/**/*.{ts,tsx}'",
    "formater": "prettier --write '{src,__{tests,mocks}__}/**/*.{ts,tsx}' --fix",
    "lint-fix": "eslint '{src,__tests__}/**/**.{ts,tsx}' --fix",
    "lint": "eslint '{src,__tests__}/**/**.{ts,tsx}' && prettier --check '{src,__{tests,mocks}__}/**/*.{ts,tsx}'",
    "postinstall": "husky install",
    "create:domain": "hygen domains new",
    "create:module": "hygen modules new"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-push": [
    "lint",
    "build"
  ],
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@hookform/resolvers": "^3.3.0",
    "@liveblocks/client": "^1.8.2",
    "@liveblocks/core": "^1.8.2",
    "@liveblocks/react": "^1.8.2",
    "@liveblocks/redux": "^1.1.0",
    "@mx/denise-core": "^1.0.20",
    "@mx/ui": "0.0.44-dev",
    "bowser": "^2.11.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "encoding": "^0.1.13",
    "firebase": "^10.3.0",
    "get-user-media-promise": "^1.1.4",
    "graphql": "^16.8.0",
    "i18next": "^23.4.5",
    "lodash": "^4.17.21",
    "peerjs": "^1.5.0",
    "postcss": "8.4.24",
    "react": "16.14.0",
    "react-collapsible": "^2.10.0",
    "react-dom": "16.14.0",
    "react-draggable": "^4.4.6",
    "react-ga4": "^2.1.0",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.45.4",
    "react-i18next": "^13.2.0",
    "react-icons": "^4.10.1",
    "react-intl": "^6.4.4",
    "react-joyride": "^2.6.1",
    "react-lottie": "^1.2.3",
    "react-quill": "^2.0.0",
    "react-redux": "5.0.7",
    "react-router-dom": "^6.15.0",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "uuid": "^9.0.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@mx/eslint": "^1.0.4",
    "@testing-library/jest-dom": "^6.1.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-lottie": "^1.2.10",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-redux": "^7.1.25",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "9.0.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "postcss-import": "^12.0.0",
    "postcss-simple-vars": "^5.0.1",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "source-map-explorer": "^2.5.3",
    "vite": "^4.3.8",
    "web-vitals": "^3.4.0"
  }
}